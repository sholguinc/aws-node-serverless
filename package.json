{
  "name": "aws-node-http-project",
  "version": "1.0.0",
  "license": "MIT",
  "packageManager": "yarn@4.1.1",
  "scripts": {
    "build": "rimraf dist && tsc",
    "offline": "serverless offline",
    "dev": "yarn build && yarn offline",
    "deploy": "serverless deploy",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier . --write",
    "project-version": "echo $npm_package_version",
    "db:up": "docker compose up mysql",
    "db:down": "docker compose down mysql",
    "typeorm": "npx typeorm-ts-node-commonjs -d src/config/db.config.ts",
    "migration:generate": "yarn typeorm migration:generate -p",
    "migration:run": "yarn typeorm migration:run",
    "migration:show": "yarn typeorm migration:show",
    "migration:revert": "yarn typeorm migration:revert"
  },
  "dependencies": {
    "axios": "^1.6.8",
    "body-parser": "^1.20.2",
    "class-validator": "^0.14.1",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "mysql": "^2.18.1",
    "reflect-metadata": "^0.2.2",
    "serverless-http": "^3.2.0",
    "translatte": "^3.0.1",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.136",
    "@types/body-parser": "^1",
    "@types/eslint": "^8",
    "@types/express": "^4.17.21",
    "@types/mysql": "^2",
    "@types/node": "^20.11.30",
    "@types/serverless": "^3",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-promise": "^6.1.1",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "serverless": "^3.38.0",
    "serverless-offline": "^13.3.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.3"
  }
}
